{
  "name": "waldo.vision",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "clean": "turbo run clean",
    "checks": "turbo run build lint",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md,json}\""
  },
  "devDependencies": {
    "eslint": "^8.29.0",
    "eslint-config-custom": "workspace:*",
    "prettier": "^2.8.1",
    "tsconfig": "workspace:*",
    "turbo": "^1.6.3",
    "typescript": "^4.9.4"
  },
  "engines": {
    "node": "^18.12.1"
  },
  "packageManager": "pnpm@7.18.1"
}
